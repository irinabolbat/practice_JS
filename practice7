let weight = 10;
let recommendation;

if(weight < 4) {
  recommendation = 'Пора перекусить';
  alert(recommendation.toString())
} else if(weight <= 5.5) {
    recommendation = 'Вес в норме';
    alert(recommendation.toString());
} else if (weight > 5.5) {
  recommendation = 'Пора на тренировку';
  alert(recommendation.toString());
};
